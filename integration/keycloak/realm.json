{
  "realm": "grafana",
  "enabled": true,
  "sslRequired": "none",
  "registrationAllowed": false,
  "requiredCredentials": [
    "password"
  ],
  "defaultSignatureAlgorithm": "RS256",
  "roles": {
    "realm": [
      {
        "name": "uma_authorization",
        "description": "${role_uma_authorization}",
        "composite": false,
        "clientRole": false
      },
      {
        "name": "default-roles-grafana",
        "description": "${role_default-roles}",
        "composite": true,
        "composites": {
          "realm": [
            "offline_access",
            "uma_authorization"
          ]
        },
        "clientRole": false
      },
      {
        "name": "offline_access",
        "description": "${role_offline-access}",
        "composite": false,
        "clientRole": false
      },
      {
        "name": "admin",
        "description": "Grafana Administrator privileges"
      },
      {
        "name": "editor",
        "description": "Grafana Editor privileges"
      },
      {
        "name": "viewer",
        "description": "Grafana Viewer privileges"
      }
    ]
  },
  "defaultRole": {
    "name": "default-roles-grafana",
    "description": "${role_default-roles}",
    "composite": true,
    "clientRole": false
  },
  "scopeMappings": [
    {
      "clientScope": "offline_access",
      "roles": [
        "offline_access"
      ]
    }
  ],
  "clients": [
    {
      "clientId": "grafana-oauth",
      "enabled": true,
      "protocol": "openid-connect",
      "clientAuthenticatorType": "client-secret",
      "secret": "1fakeGrafanaSecret00000000000000",
      "redirectUris": [
        "*"
      ],
      "webOrigins": [
        "*"
      ],
      "directAccessGrantsEnabled": true,
      "publicClient": false,
      "protocolMappers": [
        {
          "name": "roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "consentRequired": false,
          "config": {
            "multivalued": "true",
            "userinfo.token.claim": "true",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "roles",
            "jsonType.label": "String"
          }
        }
      ]
    },
    {
      "clientId": "test-server",
      "enabled": true,
      "protocol": "openid-connect",
      "clientAuthenticatorType": "client-secret",
      "secret": "1fakeTestServerSecret00000000000",
      "redirectUris": [
        "*"
      ],
      "webOrigins": [
        "*"
      ],
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "publicClient": false,
      "fullScopeAllowed": true
    }
  ],
  "users": [
    {
      "username": "user",
      "enabled": true,
      "email": "user@test.localhost",
      "emailVerified": true,
      "firstName": "Sample",
      "lastName": "User",
      "credentials": [
        {
          "type": "password",
          "value": "user"
        }
      ],
      "realmRoles": [
        "admin",
        "offline_access"
      ]
    }
  ]
}
