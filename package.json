{
  "name": "grafana-odata-datasource",
  "version": "1.0.0",
  "description": "Loads data from OData (V4) compliant data sources to Grafana",
  "bugs": {
    "url": "https://github.com/d-velop/grafana-odata-datasource/issues"
  },
  "homepage": "https://github.com/d-velop/grafana-odata-datasource#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/d-velop/grafana-odata-datasource.git"
  },
  "keywords": [
    "Grafana",
    "Datasource",
    "OData"
  ],
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "e2e": "yarn cypress install && yarn grafana-e2e run",
    "e2e:update": "yarn cypress install && yarn grafana-e2e run --update-screenshots",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "startMock": "ts-node --project mock/tsconfig-mock.json mock/index.ts"
  },
  "author": "d.velop AG",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@grafana/e2e": "^9.4.7",
    "@grafana/e2e-selectors": "^9.4.7",
    "@grafana/eslint-config": "^5.1.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@swc/core": "1.3.75",
    "@swc/helpers": "^0.4.12",
    "@swc/jest": "^0.2.23",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@types/express": "^4.17.17",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.2",
    "@types/lodash": "^4.14.194",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.11",
    "@types/uuid": "^9.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint-webpack-plugin": "^3.1.1",
    "express": "^4.18.2",
    "fork-ts-checker-webpack-plugin": "^7.2.0",
    "glob": "^8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "morgan": "^1.10.0",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.56.1",
    "sass-loader": "13.2.0",
    "style-loader": "3.3.1",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.4.0",
    "uuid": "^9.0.0",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-livereload-plugin": "^3.0.2",
    "xmlbuilder2": "^3.0.2"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@grafana/data": "^9.4.7",
    "@grafana/runtime": "^9.4.7",
    "@grafana/ui": "^9.4.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
