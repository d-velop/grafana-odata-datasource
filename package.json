{
  "name": "grafana-odata-datasource",
  "version": "1.3.0",
  "description": "Loads data from OData (V2, V3 and V4) compliant data sources to Grafana",
  "bugs": {
    "url": "https://github.com/d-velop/grafana-odata-datasource/issues"
  },
  "homepage": "https://github.com/d-velop/grafana-odata-datasource#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/d-velop/grafana-odata-datasource.git"
  },
  "keywords": [
    "Grafana",
    "Datasource",
    "OData"
  ],
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache .",
    "lint:fix": "yarn run lint --fix && prettier --write --list-different .",
    "e2e": "playwright test",
    "server": "docker compose up --build",
    "server:withKeycloak": "docker compose -f docker-compose.withKeycloak.yaml up --build",
    "checkCompatibility": "npx @grafana/levitate@latest is-compatible --path src/module.ts --target @grafana/data,@grafana/ui,@grafana/runtime",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "author": "d.velop AG",
  "license": "Apache-2.0",
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@grafana/eslint-config": "^9.0.0",
    "@grafana/plugin-e2e": "^3.4.0",
    "@grafana/tsconfig": "^2.0.1",
    "@openfeature/core": "^1.9.2",
    "@openfeature/web-sdk": "^1.7.3",
    "@playwright/test": "^1.58.2",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@swc/core": "^1.15.17",
    "@swc/helpers": "^0.5.19",
    "@swc/jest": "^0.2.39",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.3.2",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "axe-core": "^4.11.1",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^62.7.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-webpack-plugin": "^5.0.2",
    "eslint": "^9.32.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "glob": "^13.0.6",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "playwright-core": "^1.58.2",
    "prettier": "^3.8.1",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "^1.97.3",
    "sass-loader": "^16.0.7",
    "semver": "^7.7.4",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.7",
    "terser-webpack-plugin": "^5.3.16",
    "ts-node": "^10.9.2",
    "typescript": "5.9.3",
    "webpack": "^5.105.3",
    "webpack-cli": "^6.0.1",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@grafana/data": "^12.3.3",
    "@grafana/runtime": "^12.4.0",
    "@grafana/schema": "^12.4.0",
    "@grafana/ui": "^12.4.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "packageManager": "yarn@1.22.22",
  "resolutions": {
    "underscore": "^1.12.1",
    "debug": "^3.1.0",
    "log4js": "^6.4.0",
    "@cypress/request": "^3.0.0",
    "tough-cookie": "^4.1.3"
  }
}
