{
  "name": "grafana-odata-datasource-test-server",
  "version": "1.0.0",
  "description": "A simple test server for the Grafana OData Data Source Plugin",
  "scripts": {
    "start": "ts-node generateTestdata.ts && cds-ts deploy && cds-ts serve"
  },
  "author": "d.velop AG",
  "license": "Apache-2.0",
  "dependencies": {
    "@sap/cds": "^7.2.0",
    "express": "^4.18.2",
    "@types/express": "^4.17.17",
    "uuid": "^9.0.0",
    "@types/uuid": "^9.0.3",
    "xmlbuilder2": "^3.1.1",
    "sqlite3": "^5.1.6",
    "prettier": "^3.0.3",
    "typescript": "^4.3.4",
    "morgan": "^1.10.0"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "url": "db.sqlite"
        }
      }
    },
    "log": {
      "service": true
    },
    "features": {
      "odata_new_parser": true
    }
  }
}
