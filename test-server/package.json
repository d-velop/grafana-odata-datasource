{
  "name": "grafana-odata-datasource-test-server",
  "version": "1.0.0",
  "description": "A simple test server for the Grafana OData Data Source Plugin",
  "scripts": {
    "start": "ts-node generateTestdata.ts && cds-ts deploy && cds-ts serve"
  },
  "author": "d.velop AG",
  "license": "Apache-2.0",
  "dependencies": {
    "@sap/cds": "^8.8.2",
    "@types/express": "^5.0.1",
    "@types/uuid": "^10.0.0",
    "express": "^4.21.2",
    "morgan": "^1.10.0",
    "prettier": "^3.5.3",
    "sqlite3": "^5.1.7",
    "typescript": "5.8.2",
    "uuid": "^11.1.0",
    "xmlbuilder2": "^3.1.1"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "yarn@1.22.22",
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "url": "db.sqlite"
        }
      }
    },
    "log": {
      "service": true
    },
    "features": {
      "odata_new_parser": true
    }
  }
}
